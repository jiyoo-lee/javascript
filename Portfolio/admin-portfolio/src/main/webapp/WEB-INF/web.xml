<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Portfolio</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  
  <servlet>
	<servlet-name>register</servlet-name>
	<servlet-class>admin.Registeration</servlet-class>
</servlet>

<servlet-mapping>
	<servlet-name>register</servlet-name>
	<url-pattern>/admin/registeration.do</url-pattern>
</servlet-mapping>

  <servlet>
	<servlet-name>login</servlet-name>
	<servlet-class>admin.Login</servlet-class>
</servlet>

<servlet-mapping>
	<servlet-name>login</servlet-name>
	<url-pattern>/admin/login.do</url-pattern>
</servlet-mapping>

  <servlet>
	<servlet-name>setEnvironment</servlet-name>
	<servlet-class>admin.Environment</servlet-class>
</servlet>

<servlet-mapping>
	<servlet-name>setEnvironment</servlet-name>
	<url-pattern>/admin/environment.do</url-pattern>
</servlet-mapping>



</web-app>